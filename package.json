    "reason-react": "^0.5.3",
    "subscriptions-transport-ws": "^0.9.12"
  },
  "scripts": {
    "build": "bsb -make-world",
    "schema": "mix absinthe.schema.json --schema PhotoManagementApi.Web.Schema graphql_schema.json",
    "start": "concurrently \"bsb -make-world -w\" \"NODE_ENV=development webpack-dev-server --open\"",
    "start:hot": "concurrently \"bsb -make-world -w\" \"NODE_ENV=development webpack-dev-server --hot --open\"",
    "clean": "bsb -clean-world",
    "test": "bsb -make-world && jest",
    "webpack": "NODE_ENV=development webpack -w",
    "webpack:production": "bsb -clean-world && bsb -make-world && PUBLIC_URL='' NODE_ENV=production webpack && cp -Rp public/* build/ && rm -rf apps/photo_management_api_web/priv/static/* && cp -Rp build/* apps/photo_management_api_web/priv/static"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@glennsl/bs-jest": "^0.4.3",
    "autoprefixer": "^9.1.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-graphql-tag": "^1.5.0",
    "babel-plugin-named-asset-import": "^1.0.0-next.66cc7a90",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^4.0.0-next.66cc7a90",
    "brotli-webpack-plugin": "^1.0.0",
    "bs-css": "7.1.0",
    "bs-platform": "^4.0.5",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "concurrently": "^3.5.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "graphql_ppx": "^0.2.7",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "react-dev-utils": "^5.0.1",
    "style-loader": "^0.22.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "thread-loader": "^1.1.5",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-manifest-plugin": "^2.0.1",
    "webpack-null-plugin": "^0.0.2"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@glennsl|bs-platform)/)"
    ]
  }
}
