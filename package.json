{
  "name": "photo-manager-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "@svgr/webpack": "^2.2.1",
    "apollo-cache-inmemory": "^1.2.8",
    "apollo-client": "^2.4.0",
    "apollo-link": "^1.0.0",
    "apollo-link-context": "^1.0.0",
    "apollo-link-error": "^1.0.0",
    "apollo-link-http": "^1.0.0",
    "apollo-link-ws": "^1.0.8",
    "bs-dynamic-import": "^0.3.0",
    "bs-fetch": "^0.3.0",
    "glamor": "^2.20.40",
    "graphql": "^0.13.0",
    "graphql-tag": "^2.8.0",
    "raven-js": "^3.24.0",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-photoswipe": "^1.3.0",
