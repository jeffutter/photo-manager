{
  "name": "photo-manager-ui",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/__auth": {
      "target": "http://localhost:4000"
    },
    "/auth": {
      "target": "http://localhost:4000"
    },
    "/config": {
      "target": "http://localhost:4000"
    },
    "/graphql": {
      "target": "http://localhost:4000"
    },
    "/graphiql": {
      "target": "http://localhost:4000"
    }
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.0.0",
    "apollo-client": "^2.0.1",
    "apollo-link": "^1.0.0",
    "apollo-link-context": "^1.0.0",
    "apollo-link-error": "^1.0.0",
    "apollo-link-http": "^1.0.0",
    "bricks.js": "^1.8.0",
    "bs-glamor": "poeschko/bs-glamor",
    "graphql": "^0.12.0",
    "graphql-tag": "^2.5.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.0.0",
    "react-apollo": "^2.0.0",
    "react-dom": "^16.0.0",
    "react-photoswipe": "^1.3.0",
    "react-router-dom": "^4.2.2",
    "react-waypoint": "^8.0.0",
    "reason-scripts": "0.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rm -rf apps/photo_management_api_web/priv/static/* && cp -Rp build/* apps/photo_management_api_web/priv/static",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prepare": "npm link bs-platform"
  },
  "devDependencies": {
    "bs-jest": "^0.3.0",
    "reason-react": "^0.3.0"
  }
}
